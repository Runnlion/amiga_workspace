            <mass value="${smartfork_weight}" />
            <origin xyz="0 ${-1 * smartfork_wheel_distance} 0" rpy="0 0 0" />
            <inertia    ixx="${(1/12) * wheel_weight * (smartfork_height * smartfork_height + smartfork_width * smartfork_width)}" 
