<?xml version="1.0" ?>
<robot name="robot_with_wheels" xmlns:xacro="http://ros.org/wiki/xacro">
    
    <!-- Define mateiral RGBA -->
    <material name="dark_grey">
        <color rgba="0.2 0.2 0.2 1.0"/>
    </material>
    <material name="light_grey">
        <color rgba="0.4 0.4 0.4 1.0"/>
    </material>
    <material name="yellow">
        <color rgba="0.8 0.8 0.0 1.0"/>
    </material>
    <material name="black">
        <color rgba="0.15 0.15 0.15 1.0"/>
    </material>
    <material name="aluminum">
        <color rgba="0.8 0.85 0.88 1"/>
    </material>
    <material name="white">
        <color rgba="1.0 1.0 1.0 1.0"/>
    </material>

    <xacro:property name="wheel_weight" value="3.0" />
    <xacro:property name="wheel_radius" value="0.215" />
    <xacro:property name="wheel_width" value="0.14" />
    
    <!-- Verify this to get a precise data -->
    <xacro:property name="smartfork_weight" value="10" />
    <xacro:property name="smartfork_width" value="0.2" /> 
    <xacro:property name="smartfork_height" value="0.395" /> 
    <xacro:property name="smartfork_length" value="0.3" /> 
    <xacro:property name="smartfork_wheel_distance" value="0.395" /> 


    <link name="base_link"/>
    <joint name="base_link_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0.5 0 0"/>
        <parent link="base_link"/>
        <child link="chassis_link"/>
    </joint>

    <link name="chassis_link" />
    <!-- Define origin for the front_center frame -->
    <joint name="world_joint" type="fixed">
        <origin xyz="0 0.0 0.65" rpy="0 0 0" />
        <parent link="chassis_link" />
        <child link="front_center" />
    </joint>

    <link name="front_center" />
    <!-- Define origin for the front_center frame -->
    <joint name="front_center_to_smartfork3_joint" type="fixed">
        <origin xyz="0 -0.5 0" rpy="1.5708 0 0" />
        <parent link="front_center" />
        <child link="smartfork3" />
